<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.calosize">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CaloSize">
        <activity android:name=".YourProfile"></activity>
        <activity android:name=".Settings" />
        <activity android:name=".UnderMaintenance" />
        <activity android:name=".BMIResult" />
        <activity android:name=".BMIMainActivity" />
        <activity android:name=".CalorieCalculator" />
        <activity android:name=".HomePageActivity" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>